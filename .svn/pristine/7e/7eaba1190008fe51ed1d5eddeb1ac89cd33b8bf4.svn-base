<?xml version="1.0" encoding="utf-8"?>
<FrameLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:textcircle="http://schemas.android.com/apk/res-auto"
     android:layout_width="match_parent"
     android:layout_height="match_parent"
    >
<RelativeLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fffffe"
    android:orientation="vertical" >
    <LinearLayout 
        android:layout_width="match_parent"
    	android:layout_height="wrap_content"
    	android:orientation="vertical">
	    <RelativeLayout
	        android:layout_width="fill_parent"
	        android:layout_height="48dip"
	        android:background="@drawable/action_bar_bg" >
			<ImageView
              	android:id="@+id/back"
	            android:layout_width="wrap_content"
	            android:layout_height="fill_parent"
	            android:layout_alignParentLeft="true"
	            android:layout_margin="10dp"
	            android:src="@drawable/left" />
	        <TextView
	            android:layout_width="wrap_content"
	            android:layout_height="fill_parent"
	            android:layout_toRightOf="@id/back"
	            android:gravity="center"
	            android:text="日程详情"
	            android:textColor="#90fffffe"
	            android:textSize="16sp" />
	  
	    </RelativeLayout>
	    
	    <RelativeLayout
	        android:layout_width="fill_parent"
       		android:layout_height="52dip" 
       		android:layout_marginLeft="12dip"
       		android:layout_marginRight="12dip">
	        <TextView 
	             android:id="@+id/schedule_title"
	             android:layout_width="fill_parent"
	       		 android:layout_height="match_parent"
	       		 android:layout_marginTop="12dip"
	       		 android:layout_marginLeft="10dip"
	       		 android:layout_marginRight="12dip"
	       		 android:textSize="16sp"
	       		 android:paddingLeft="8dip"
	       		 android:gravity="center_vertical"/>
	        <View
		        android:layout_alignParentBottom="true"
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#20000000" />
	     </RelativeLayout>
	    <RelativeLayout
	        android:layout_width="fill_parent"
       		android:layout_height="72dip" 
       		android:layout_marginLeft="12dip"
       		android:layout_marginRight="12dip">
	        <TextView 
	             android:id="@+id/schedule_content"
	             android:layout_width="fill_parent"
	       		 android:layout_height="match_parent"
	       		 android:layout_marginTop="12dip"
	       		 android:layout_marginLeft="10dip"
	       		 android:layout_marginRight="12dip"
	       		 android:textSize="16sp"
	       		 android:paddingLeft="8dip"
	       		 android:gravity="center_vertical"/>
	        <View
		        android:layout_alignParentBottom="true"
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#20000000" />
	     </RelativeLayout>
	     <RelativeLayout 
	            android:layout_width="fill_parent"
	       		android:layout_height="52dip" 
	       		android:layout_marginLeft="12dip"
	       		android:layout_marginRight="12dip">
	           <TextView 
	               android:id="@+id/schedule_time"
	               android:layout_width="wrap_content"
	               android:layout_height="fill_parent"
	               android:layout_alignParentLeft="true"
	               android:layout_marginLeft="8dip"
	               android:textSize="16sp"
	               android:text="时间"
	               android:gravity="center"
	               android:textColor="#cc000000"
	               />
	           <TextView 
	                android:id="@+id/scheduletime"
	               android:layout_width="wrap_content"
	               android:layout_height="fill_parent"
	               android:layout_toRightOf="@id/schedule_time"
	               android:layout_marginLeft="20dip"
	               android:textSize="14sp"
	               android:gravity="center"
	               android:textColor="#80000000"
	               />
	             <View
			        android:layout_alignParentBottom="true"
		            android:layout_width="fill_parent"
		            android:layout_height="1px"
		            android:background="#20000000" />
	    </RelativeLayout>
	     <RelativeLayout 
	            android:layout_width="fill_parent"
	       		android:layout_height="52dip" 
	       		android:layout_marginLeft="12dip"
	       		android:layout_marginRight="12dip">
	           <TextView 
	               android:id="@+id/job_type"
	               android:layout_width="wrap_content"
	               android:layout_height="fill_parent"
	               android:layout_alignParentLeft="true"
	               android:layout_marginLeft="8dip"
	               android:textSize="16sp"
	               android:text="工作类型"
	               android:gravity="center"
	               android:textColor="#cc000000"
	               />
	           <TextView 
	                android:id="@+id/jobtype_value"
	               android:layout_width="wrap_content"
	               android:layout_height="fill_parent"
	               android:layout_marginRight="20dip"
	               android:layout_alignParentRight="true"
	               android:textSize="14sp"
	               android:gravity="center"
	               android:textColor="#80000000"
	               />
	             <View
			        android:layout_alignParentBottom="true"
		            android:layout_width="fill_parent"
		            android:layout_height="1px"
		            android:background="#20000000" />
	    </RelativeLayout>
	     <RelativeLayout 
	            android:layout_width="fill_parent"
	       		android:layout_height="52dip" 
	       		android:layout_marginLeft="12dip"
	       		android:layout_marginRight="12dip">
	           <TextView 
	               android:id="@+id/status"
	               android:layout_width="wrap_content"
	               android:layout_height="fill_parent"
	               android:layout_alignParentLeft="true"
	               android:layout_marginLeft="8dip"
	               android:textSize="16sp"
	               android:text="状态"
	               android:gravity="center"
	               android:textColor="#cc000000"
	               />
	           <TextView 
	                android:id="@+id/status_value"
	               android:layout_width="wrap_content"
	               android:layout_height="fill_parent"
	               android:layout_marginRight="20dip"
	               android:layout_alignParentRight="true"
	               android:textSize="14sp"
	               android:gravity="center"
	               android:textColor="#80000000"
	               />
	             <View
			        android:layout_alignParentBottom="true"
		            android:layout_width="fill_parent"
		            android:layout_height="1px"
		            android:background="#20000000" />
	    </RelativeLayout>
    </LinearLayout>
    <RelativeLayout
        android:layout_alignParentBottom="true"
        android:layout_width="fill_parent"
        android:layout_height="60dip" >

        <View
            android:id="@+id/center_circle_view"
            android:layout_width="15dip"
            android:layout_height="1px"
            android:layout_centerInParent="true">
        </View>
        <com.cn7782.management.android.activity.tabview.CircleTextView
            android:id="@+id/edit_circle_view"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:layout_centerInParent="true"
            android:layout_toRightOf="@id/center_circle_view"
            textcircle:text="编辑" />
        <com.cn7782.management.android.activity.tabview.CircleTextView
            android:id="@+id/delete_circle_view"
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:layout_centerInParent="true"
            android:layout_toLeftOf="@id/center_circle_view"
            textcircle:text="删除" />
    </RelativeLayout>
</RelativeLayout>
<RelativeLayout
    android:id="@+id/delete_show_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#80000000"
    android:visibility="gone"
    
    >
    <include layout="@layout/delete_dialog_layout" />
</RelativeLayout>
    
</FrameLayout>